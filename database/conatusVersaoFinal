-- --------------------------------------------------------

--
-- Estrutura para view `nomeUsuarioPost`
--
DROP TABLE IF EXISTS `nomeUsuarioPost`;

CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `nomeUsuarioPost`  AS  select `usuario`.`nome_user` AS `nome_user`,`post`.`titlepost` AS `titlepost`,`post`.`categoria` AS `categoria` from (`usuario` join `post` on((`usuario`.`id_user` = `post`.`id_user`))) ;

