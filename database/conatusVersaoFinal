-- --------------------------------------------------------

--
-- Estrutura para tabela `usuario`
--

CREATE TABLE `usuario` (
  `nome` varchar(25) NOT NULL,
  `sobrenome` varchar(30) NOT NULL,
  `data_nascimento` date DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

-- --------------------------------------------------------

--
-- Estrutura stand-in para view `viewaniversariante`
-- (Veja abaixo para a visão atual)
--
CREATE TABLE `viewaniversariante` (
`data_nascimento` date
,`nome` varchar(25)
,`sobrenome` varchar(30)
);

-- --------------------------------------------------------

--
-- Estrutura para view `viewaniversariante`
--
DROP TABLE IF EXISTS `viewaniversariante`;

CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `viewaniversariante` (`nome`, `sobrenome`, `data_nascimento`) AS   select `usuario`.`nome` AS `nome`,`usuario`.`sobrenome` AS `sobrenome`,`usuario`.`data_nascimento` AS `data_nascimento` from `usuario`  ;

--
-- Índices de tabelas apagadas
--

--
-- Índices de tabela `aluno`
--
ALTER TABLE `aluno`
  ADD PRIMARY KEY (`id`);

--
-- Índices de tabela `alunos`
--
ALTER TABLE `alunos`
  ADD PRIMARY KEY (`idalunos`);

--
-- Índices de tabela `case1`
--
ALTER TABLE `case1`
  ADD PRIMARY KEY (`ID_NF`,`ID_ITEM`);

--
-- Índices de tabela `curso`
--
ALTER TABLE `curso`
  ADD PRIMARY KEY (`codC`);

--
-- Índices de tabela `disciplina`
--
ALTER TABLE `disciplina`
  ADD PRIMARY KEY (`codD`),
  ADD UNIQUE KEY `nome` (`nomeD`),
  ADD KEY `ind_carga_horaria` (`cargaD`);

--
-- Índices de tabela `grade`
--
ALTER TABLE `grade`
  ADD PRIMARY KEY (`cod_cur`,`cod_dis`,`cod_pro`),
  ADD KEY `cod_dis` (`cod_dis`),
  ADD KEY `cod_pro` (`cod_pro`);

--
-- Índices de tabela `pessoa`
--
ALTER TABLE `pessoa`
  ADD PRIMARY KEY (`idpessoa`,`nome`),
  ADD KEY `idx_name` (`nome`);

--
-- Índices de tabela `Produtos`
--
ALTER TABLE `Produtos`
  ADD PRIMARY KEY (`Referencia`),
  ADD UNIQUE KEY `Descricao` (`Descricao`);

--
-- Índices de tabela `professor`
--
ALTER TABLE `professor`
  ADD PRIMARY KEY (`cod_pro`);

--
-- Índices de tabela `tbl_Clientes`
--
ALTER TABLE `tbl_Clientes`
  ADD PRIMARY KEY (`id_cliente`);

--
-- Índices de tabela `tbl_Produtos`
--
ALTER TABLE `tbl_Produtos`
  ADD PRIMARY KEY (`id_produto`);

--
-- Índices de tabela `tbl_Vendas`
--
ALTER TABLE `tbl_Vendas`
  ADD PRIMARY KEY (`id_venda`),
  ADD KEY `fk_tbl_Vendas_1_idx` (`id_produto`),
  ADD KEY `fk_tbl_Vendas_2_idx` (`id_cliente`);

--
-- Índices de tabela `telefone`
--
ALTER TABLE `telefone`
  ADD PRIMARY KEY (`idtelefone`),
  ADD KEY `fk_telefone_1_idx` (`id_aluno`);

--
-- Índices de tabela `telefones`
--
ALTER TABLE `telefones`
  ADD PRIMARY KEY (`idtefefone`,`idpessoatelefone`),
  ADD KEY `fk_pessoa_telefone_idx` (`idpessoatelefone`);

--
-- AUTO_INCREMENT de tabelas apagadas
--

--
-- AUTO_INCREMENT de tabela `aluno`
--
ALTER TABLE `aluno`
  MODIFY `id` int NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=7;

--
-- AUTO_INCREMENT de tabela `alunos`
--
ALTER TABLE `alunos`
  MODIFY `idalunos` int NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=8;

--
-- AUTO_INCREMENT de tabela `pessoa`
--
ALTER TABLE `pessoa`
  MODIFY `idpessoa` int NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=6;

--
-- AUTO_INCREMENT de tabela `telefone`
--
ALTER TABLE `telefone`
  MODIFY `idtelefone` int NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=5;

--
-- AUTO_INCREMENT de tabela `telefones`
--
ALTER TABLE `telefones`
  MODIFY `idtefefone` int NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=6;

--
-- Restrições para dumps de tabelas
--

--
-- Restrições para tabelas `grade`
--
ALTER TABLE `grade`
  ADD CONSTRAINT `grade_ibfk_1` FOREIGN KEY (`cod_cur`) REFERENCES `curso` (`codC`),
  ADD CONSTRAINT `grade_ibfk_2` FOREIGN KEY (`cod_dis`) REFERENCES `disciplina` (`codD`),
  ADD CONSTRAINT `grade_ibfk_3` FOREIGN KEY (`cod_pro`) REFERENCES `professor` (`cod_pro`);

--
-- Restrições para tabelas `tbl_Vendas`
--
ALTER TABLE `tbl_Vendas`
  ADD CONSTRAINT `fk_tbl_Vendas_1` FOREIGN KEY (`id_produto`) REFERENCES `tbl_Produtos` (`id_produto`),
  ADD CONSTRAINT `fk_tbl_Vendas_2` FOREIGN KEY (`id_cliente`) REFERENCES `tbl_Clientes` (`id_cliente`);

--
-- Restrições para tabelas `telefone`
--
ALTER TABLE `telefone`
  ADD CONSTRAINT `fk_telefone_1` FOREIGN KEY (`id_aluno`) REFERENCES `aluno` (`id`);

--
-- Restrições para tabelas `telefones`
--
ALTER TABLE `telefones`
  ADD CONSTRAINT `fk_pessoa_telefone` FOREIGN KEY (`idpessoatelefone`) REFERENCES `pessoa` (`idpessoa`);
--
-- Banco de dados: `secretaria`
--
CREATE DATABASE IF NOT EXISTS `secretaria` DEFAULT CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci;
USE `secretaria`;

-- --------------------------------------------------------

--
-- Estrutura para tabela `aluno`
--

CREATE TABLE `aluno` (
  `id_aluno` int NOT NULL,
  `nome` varchar(50) DEFAULT NULL,
  `idade` int DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

